{
    "version": 2,
    "builds": [
      {
        "src": "app.py",
        "use": "@vercel/python"
      },
      {
        "src": "*.html",
        "use": "@vercel/static"
      },
      {
        "src": "*.jpg",
        "use": "@vercel/static"
      },
      {
        "src": "*.jpeg",
        "use": "@vercel/static"
      },
      {
        "src": "*.png",
        "use": "@vercel/static"
      },
      {
        "src": "*.gif",
        "use": "@vercel/static"
      },
      {
        "src": "*.svg",
        "use": "@vercel/static"
      },
      {
        "src": "*.webp",
        "use": "@vercel/static"
      },
      {
        "src": "images/*",
        "use": "@vercel/static"
      }
    ],
    "routes": [
      {
        "src": "/send-message",
        "dest": "app.py",
        "methods": ["POST", "OPTIONS"],
        "headers": {
          "Access-Control-Allow-Origin": "*",
          "Access-Control-Allow-Methods": "POST, OPTIONS",
          "Access-Control-Allow-Headers": "Content-Type, Accept"
        }
      },
      {
        "src": "/images/(.*\\.(jpg|jpeg|png|gif|svg|webp))",
        "dest": "/images/$1",
        "headers": {
          "cache-control": "public, max-age=31536000, immutable"
        }
      },
      {
        "src": "/(.*\\.(jpg|jpeg|png|gif|svg|webp))",
        "dest": "/$1",
        "headers": {
          "cache-control": "public, max-age=31536000, immutable"
        }
      },
      {
        "src": "/(.*)",
        "dest": "$1"
      }
    ]
  }
  